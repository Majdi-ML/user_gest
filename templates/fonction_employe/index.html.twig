{% extends 'base.html.twig' %}

{% block title %}FonctionEmploye index{% endblock %}

{% block body %}
    <h1>FonctionEmploye index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fonction_employe in fonction_employes %}
            <tr>
                <td>{{ fonction_employe.id }}</td>
                <td>{{ fonction_employe.libelle }}</td>
                <td>
                    <a href="{{ path('app_fonction_employe_show', {'id': fonction_employe.id}) }}">show</a>
                    <a href="{{ path('app_fonction_employe_edit', {'id': fonction_employe.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_fonction_employe_new') }}">Create new</a>
{% endblock %}
