{% extends 'base.html.twig' %}

{% block title %}NaturePaiement index{% endblock %}

{% block body %}
    <h1>NaturePaiement index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nature</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for nature_paiement in nature_paiements %}
            <tr>
                <td>{{ nature_paiement.id }}</td>
                <td>{{ nature_paiement.nature }}</td>
                <td>
                    <a href="{{ path('app_nature_paiement_show', {'id': nature_paiement.id}) }}">show</a>
                    <a href="{{ path('app_nature_paiement_edit', {'id': nature_paiement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_nature_paiement_new') }}">Create new</a>
{% endblock %}
