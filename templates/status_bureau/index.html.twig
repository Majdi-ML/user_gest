{% extends 'base.html.twig' %}

{% block title %}StatusBureau index{% endblock %}

{% block body %}
    <h1>StatusBureau index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for status_bureau in status_bureaus %}
            <tr>
                <td>{{ status_bureau.id }}</td>
                <td>{{ status_bureau.libelle }}</td>
                <td>
                    <a href="{{ path('app_status_bureau_show', {'id': status_bureau.id}) }}">show</a>
                    <a href="{{ path('app_status_bureau_edit', {'id': status_bureau.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_status_bureau_new') }}">Create new</a>
{% endblock %}
